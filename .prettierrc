{
    "importOrder": [
        "^../(.*)$",
        "^./(.*)$",
        "^core/(.*)$",
        "^assets/(.*)$",
        "^(modules|components)/(.*)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "tabWidth": 4,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "max_line_length": 150,
    "printWidth": 150,
    "bracketSpacing": true
}
